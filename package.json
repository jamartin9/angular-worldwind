{
  "name": "angular-worldwind",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "build": "ng build --target=production -aot true -sm false --environment prod && npm run sw",
    "ng": "ng",
    "start": "npm run build && ng serve",
    "test": "ng test",
    "lint":"ng lint",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "sw": "sw-precache --root=public --config=sw-precache-config.js",
    "static-serve": "cd public && live-server --port=4200 --host=localhost --entry-file=/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/forms": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "primeng": "^2.0.0",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.0",
    "@angular/compiler-cli": "^2.4.8",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",    
    "live-server": "^1.2.0",
    "protractor": "~5.1.0",
    "sw-precache": "^5.0.0",
    "ts-node": "1.2.1",
    "tslint": "~4.4.2",
    "typescript": "^2.2.0"
  },
  "author": "Justin <DoNotEmailMe@email.com>",
  "main": "src/main.ts",
  "repository": "git@gitlab.com:spaz/angular-cesium.git"
}
